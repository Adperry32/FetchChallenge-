<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="6324">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="20.44"
            motion:maxVelocity="3.42" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/purple_dog"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="289dp"
            android:layout_height="438dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="1"
            android:layout_marginBottom="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginTop="24dp"
            android:layout_marginStart="24dp"
            motion:layout_constraintVertical_bias="0.497" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="280dp"
            android:layout_height="444dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.51"
            motion:layout_constraintHorizontal_bias="0.493"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/golden_dog"
            android:layout_marginBottom="24dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:alpha="0"
            android:scaleX="0"
            android:scaleY="0" />
        <Constraint
            motion:layout_editor_absoluteY="-5dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="0dp"
            android:layout_width="match_parent"
            android:id="@+id/fetch_title" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/purple_dog"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="295dp"
            android:layout_height="548dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="273dp"
            android:layout_height="487dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.566"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/golden_dog"
            android:layout_marginBottom="24dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:scaleX="1"
            android:scaleY="1"
            android:alpha="1" />
    </ConstraintSet>
</MotionScene>